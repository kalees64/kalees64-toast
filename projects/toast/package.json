{
  "name": "@kalees64/toast",
  "version": "0.0.2",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/collection.json ../../dist/toast/"
  },
  "ng-add": {
    "save": "true"
  },
  "schematics": "./schematics/collection.json",
  "keywords": [
    "angular",
    "angular-toast",
    "angular-alert",
    "angular-sweetalert",
    "typescript",
    "@kalees64",
    "kalees64",
    "sweetalert2"
  ],
  "peerDependencies": {
    "@angular/common": "^18.2.0",
    "@angular/core": "^18.2.0",
    "ngx-toastr": "^19.0.0",
    "tailwindcss": "^3.4.15"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false
}
